<?xml version="1.0" ?>
<edmx:Edmx xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx" Version="1.0">
    <edmx:DataServices m:DataServiceVersion="1.0" xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata">
        <Schema Namespace="default" xmlns="http://schemas.microsoft.com/ado/2008/09/edm">
            <EntityType Name="User">
                <Key>
                    <PropertyRef Name="Id"/>
                </Key>
                <Property Name="Birthdate" Type="Edm.DateTime"/>
                <Property Name="Email" Type="Edm.String"/>
                <Property Name="FirstName" Type="Edm.String"/>
                <Property Name="Id" Type="Edm.Int64" Nullable="false"/>
                <Property Name="LastName" Type="Edm.String"/>
                <Property Name="Password" Type="Edm.String"/>
                <Property Name="Role" Type="Edm.String"/>
                <NavigationProperty Name="PetDetails" Relationship="default.User_Pet_One_Many0"
                                    FromRole="User" ToRole="Pet"/>
            </EntityType>
            <EntityType Name="Pet">
                <Key>
                    <PropertyRef Name="Id"/>
                </Key>
                <Property Name="Birthdate" Type="Edm.DateTime"/>
                <Property Name="Id" Type="Edm.Int64" Nullable="false"/>
                <Property Name="Name" Type="Edm.String"/>
                <Property Name="Owner" Type="Edm.Int64" Nullable="true"/>
                <Property Name="PetType" Type="Edm.String"/>
                <NavigationProperty Name="UserDetails" Relationship="default.User_Pet_One_Many0"
                                    FromRole="Pet" ToRole="User"/>
            </EntityType>
            <EntityType Name="Dog">
                <Key>
                    <PropertyRef Name="Id"/>
                </Key>
                <Property Name="Birthdate" Type="Edm.DateTime"/>
                <Property Name="Id" Type="Edm.Int64" Nullable="false"/>
                <Property Name="IsGuideDog" Type="Edm.Boolean"/>
                <Property Name="Name" Type="Edm.String"/>
                <Property Name="Owner" Type="Edm.Int64" Nullable="true"/>
                <Property Name="PetType" Type="Edm.String"/>
            </EntityType>
            <EntityType Name="Cat">
                <Key>
                    <PropertyRef Name="Id"/>
                </Key>
                <Property Name="Birthdate" Type="Edm.DateTime"/>
                <Property Name="Id" Type="Edm.Int64" Nullable="false"/>
                <Property Name="IsBold" Type="Edm.Boolean"/>
                <Property Name="Name" Type="Edm.String"/>
                <Property Name="Owner" Type="Edm.Int64" Nullable="true"/>
                <Property Name="PetType" Type="Edm.String"/>
            </EntityType>
            <Association Name="User_Pet_One_Many0">
                <End Type="default.User" Multiplicity="0..1" Role="User"/>
                <End Type="default.Pet" Multiplicity="*" Role="Pet"/>
                <ReferentialConstraint>
                    <Principal Role="User">
                        <PropertyRef Name="Id"/>
                    </Principal>
                    <Dependent Role="Pet">
                        <PropertyRef Name="Owner"/>
                    </Dependent>
                </ReferentialConstraint>
            </Association>
            <EntityContainer Name="defaultContainer" m:IsDefaultEntityContainer="true">
                <EntitySet Name="Users" EntityType="default.User"/>
                <EntitySet Name="Pets" EntityType="default.Pet"/>
                <EntitySet Name="Dogs" EntityType="default.Dog"/>
                <EntitySet Name="Cats" EntityType="default.Cat"/>
                <AssociationSet Name="User_Pet_One_Many0Set" Association="default.User_Pet_One_Many0">
                    <End EntitySet="Users" Role="User"/>
                    <End EntitySet="Pets" Role="Pet"/>
                </AssociationSet>
            </EntityContainer>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>